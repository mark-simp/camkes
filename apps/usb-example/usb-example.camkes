#include <camkes-single-threaded.h>
#include <usb_devices.h>

component Test {
    single_threaded_component()

    HARDWARE_USB_INTERFACES

    composition {
        HARDWARE_USB_COMPOSITION
    }

    configuration {
        HARDWARE_USB_CONFIGURATION
    }
}

assembly {
    composition {
        component Test test;
    }
    configuration {
        test.dma_pool = 32 * 1024;
    }
}
